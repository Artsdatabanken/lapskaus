    "material-ui-icons": "^1.0.0-beta.17",
    "material-ui-next": "npm:material-ui@next",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-responsive": "^4.0.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
